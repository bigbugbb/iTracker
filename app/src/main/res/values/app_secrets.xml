<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--
        The client uses it to encode the google sign in account info into a jwt and send the jwt to the api.
        The api uses it to decode the jwt from client, and handle the jwt for authentication.
    -->
    <string name="google_session_jwt_secret">7dce7fccb2ee00e879dae7ef0716a053</string>

    <!--
        Ejabberd server needs both account name and password to register a new user or login.
        There are no input password if the user chooses to sign in with google.
        In this case, the app encode the account name with this secret to generate a password automatically.
    -->
    <string name="ejabberd_account_secret">48eb600958d8d5f9bc35ead4f08fb4a9</string>
</resources>