    <declare-styleable name="LowSamplingSummaryView">
        <!--<attr name="borderDrawable" format="reference" />-->
        <!--<attr name="imagePaddingTop" format="integer" />-->
        <!--<attr name="imagePaddingBottom" format="integer" />-->
    </declare-styleable>

    <declare-styleable name="GLMotionsView">
        <attr name="graphPageSize" format="float"/>
    </declare-styleable>

    <declare-styleable name="MotionsView">
        <attr name="graphPageCount" format="float"/>
        <attr name="vertCalibrationCount" format="integer"/>
        <attr name="horiCalibrationCount" format="integer"/>
        <attr name="baselineHeight" format="integer"/>
    </declare-styleable>

    <declare-styleable name="HorizontalListView">
        <attr name="android_divider" format="reference"/>
        <attr name="dividerWidth" format="integer"/>
    </declare-styleable>

    <declare-styleable name="SlideBottomPanel">
        <attr name="sbp_background_layout" format="reference"/>
        <attr name="sbp_title_height_no_display" format="dimension"/>
        <attr name="sbp_panel_height" format="dimension"/>
        <attr name="sbp_boundary" format="boolean"/>
        <attr name="sbp_move_distance_trigger" format="dimension"/>
        <attr name="sbp_animation_duration" format="integer"/>
        <attr name="sbp_fade" format="boolean"/>
        <attr name="sbp_hide_panel_title" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="TimelinesView">
        <attr name="bottomBarHeight" format="integer"/>
        <attr name="bottomBarBackground" format="reference"/>
        <attr name="overScrollSize" format="integer"/>
        <attr name="overScrollBackground" format="reference"/>
    </declare-styleable>

    <declare-styleable name="TimelineItem">
        <attr name="drawableWidth" format="integer"/>
        <attr name="drawableHeight" format="integer"/>
        <attr name="drawablePosition">
            <flag name="left" value="0"/>
            <flag name="right" value="1"/>
        </attr>
        <attr name="lineWidth" format="integer"/>
        <attr name="circleNodeRadius" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CollectionView">
        <attr name="internalPadding" format="dimension"/>
        <attr name="contentTopClearance" format="dimension"/>
    </declare-styleable>

    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference"/>
        <attr name="metaButtonBarButtonStyle" format="reference"/>
    </declare-styleable>

    <attr name="scaleType" format="enum">
        <enum name="scaleToFit" value="0" />
        <enum name="crop" value="1" />
    </attr>

    <declare-styleable name="TrackerVideoView">
        <attr name="scaleType" />
    </declare-styleable>

    <declare-styleable name="NumberPickerPreference">
        <attr name="min" format="integer" />
        <attr name="max" format="integer" />
        <attr name="positive_button_text" format="reference" />
        <attr name="negative_button_text" format="reference" />
        <attr name="dialog_layout" format="reference" />
    </declare-styleable>

    <declare-styleable name="ContactList">
        <attr name="toolbar_popup_theme" format="reference" />
        <attr name="recent_chats_background" format="reference" />
        <attr name="chat_background" format="reference" />
        <attr name="chat_input_background" format="reference" />
        <attr name="chat_input_button_color" format="reference" />
        <attr name="chat_send_button_color" format="reference" />
        <attr name="chat_incoming_balloon" format="reference" />
        <attr name="chat_incoming_balloon_pressed" format="reference" />

        <attr name="contact_list_background" format="reference" />

        <attr name="contact_list_account_group_background" format="reference" />
        <attr name="contact_list_account_group_separator" format="reference" />
        <attr name="contact_list_account_group_second_line_text_color" format="reference" />
        <attr name="contact_list_account_group_small_right_icon_color" format="reference" />
        <attr name="contact_list_account_group_small_right_text_color" format="reference" />
        <attr name="contact_list_account_group_status_icon_separator_color" format="reference" />

        <attr name="contact_list_subgroup_background" format="reference" />
        <attr name="contact_list_subgroup_separator" format="reference" />
        <attr name="contact_list_subgroup_offline_indicator_color" format="reference" />
        <attr name="contact_list_subgroup_expander_color" format="reference" />

        <attr name="contact_list_contact_background" format="reference" />
        <attr name="contact_list_contact_separator" format="reference" />
        <attr name="contact_list_contact_name_text_color" format="reference" />
        <attr name="contact_list_contact_muc_private_chat_name_text_color" format="reference" />
        <attr name="contact_list_contact_second_line_text_color" format="reference" />
        <attr name="contact_list_contact_client_large_icon_color" format="reference" />
        <attr name="contact_list_contact_small_right_icon_alpha" format="reference" />
        <attr name="contact_list_muc_indicator_color" format="reference" />

        <attr name="contact_list_active_chat_subgroup_background" format="reference" />
        <attr name="contact_list_active_chat_background" format="reference" />
        <attr name="contact_list_active_chat_text_color" format="reference" />
        <attr name="contact_list_active_chat_separator" format="reference" />
        <attr name="contact_list_active_chat_client_large_icon_color" format="reference" />

        <attr name="account_main_color" format="reference" />
        <attr name="account_status_bar_color" format="reference" />
        <attr name="account_text_color" format="reference" />

        <attr name="navigation_drawer_background" format="reference" />

        <attr name="conference_filter_input_background" format="reference" />

